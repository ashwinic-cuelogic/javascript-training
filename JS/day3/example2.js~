'use strict';
function Student( name, branch ) {
  this.name = name;
  this.branch = branch;
}
Student.prototype.getName = function() {
  return 'Name : - ' + this.name ;
}

ryan = new Student( 'Ryan', 'Computers');
console.log(ryan.getName());
mark = new Student( 'Mark', 'Computers');
console.log(mark.getName());
